# Google OAuth Example Configuration
# Copy this file to .env and fill in your values

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-should-be-long-and-random

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id-from-developer-console
GOOGLE_CLIENT_SECRET=your-google-client-secret-from-developer-console
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/oauth/google/callback

# App Configuration
PORT=3000
NODE_ENV=development
APP_NAME=AuthXExample 